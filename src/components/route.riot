<route>
  <div id={ `component-${ props.component.name }` } />
  <script>
    import { component } from 'riot'

    export default {
      onMounted() {
        console.log(this.props.component)
        const context = this.$(`#component-${this.props.component.name}`)
        console.log(context)
        const Component = component(this.props.component)
        console.log(Component)
        Component(context)
      }
    }    
  </script>
</route>